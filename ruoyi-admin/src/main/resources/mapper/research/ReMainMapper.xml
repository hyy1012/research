    <sql id="selectReMainVo">
        select id, firm_name, re_addr, postcode, header, header_phone, liaison, liaison_phone, email, wx, phone, business_scope, emp_count, firm_type, is_hyxhhy, is_hyxhhy_other, hyxhhy_other_name, has_certificate, certificate_name, certificate_level, certificate_indate, member_need, firm, people, sub_time from re_main
