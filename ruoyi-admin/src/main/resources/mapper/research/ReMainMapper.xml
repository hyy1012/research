    <select id="selectReMainList" parameterType="ReMain" resultMap="ReMainResult">
        <include refid="selectReMainVo"/>
        <where>  
            <if test="firmName != null  and firmName != ''"> and firm_name like concat('%', #{firmName}, '%')</if>
            <if test="reAddr != null  and reAddr != ''"> and re_addr = #{reAddr}</if>
            <if test="postcode != null "> and postcode = #{postcode}</if>
